{   
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "import"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "next"
    ],
    "rules": {
        "react/prop-types": "off",
        "no-unused-vars": "off",
        "@next/next/no-img-element": "off"
    }
  }